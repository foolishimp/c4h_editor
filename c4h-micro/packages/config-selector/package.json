{
  "name": "shared",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "main": "./dist/build/index.js",
  "types": "./dist/build/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/build/index.d.ts",
        "default": "./dist/build/index.js"
      },
      "require": {
        "types": "./dist/build/index.d.ts",
        "default": "./dist/build/index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/build/*.d.ts",
        "default": "./dist/build/*.js"
      },
      "require": {
        "types": "./dist/build/*.d.ts",
        "default": "./dist/build/*.js"
      }
    }
  },
  "files": [
    "dist/build"
  ],
  "scripts": {
    "dev": "echo \"No dev script for shared package\" && exit 0",
    "build": "tsc --project tsconfig.json",
    "preview": "echo \"No preview script for shared package\" && exit 0"
  },
  "dependencies": {
      "axios": "^1.6.7",
      "js-yaml": "^4.1.0",
      "date-fns": "^3.6.0",
      "shared": "workspace:*",
      "yaml-editor": "file:../yaml-editor"
  },
  "devDependencies": {
    "@types/diff": "^5.0.8",
    "@types/node": "^22.14.1",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@mui/material": "^5.15.14",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}